      "ckpt_used": false,
      "viewIsOn" : true
    }
  },
  "lenet": {
    "trainer": {
      "train_num_epoch" : 5
    },
    "optimizer": {
      "optimizer": "rmsprop"
    },
    "initializer": {
      "initializer": "xavier",
      "init_sigma": 0.01,
      "init_bias": 0.0
    },
    "net": {
      "activation": "sigmoid",
      "resize": 0,
      "input_dim_x" : 28,
      "input_dim_y" : 28,
      "input_channels" : 1,
      "conv1_channels" : 6,
      "conv1_kernel_size" : 5,
      "conv1_strides" : 1,
      "conv1_padding" : 0,
      "pool1_size" : 2,
      "pool1_strides" : 2,
      "pool1_padding" : 0,
      "conv2_channels" : 16,
      "conv2_kernel_size" : 5,
      "conv2_striders" : 1,
      "conv2_padding" : 0,
      "pool2_size" : 2,
      "pool2_strides" : 2,
      "pool2_padding" : 0,
      "dense1_hiddens" : 120,
      "dense2_hiddens" : 84,
      "dense3_hiddens" : 10,
      "class_num" : 10
    }
  },

  "rnn": {
    "#randomIterIsOn": "#当randomIterIsOn=True时读取数据时采用shuffle方式",
